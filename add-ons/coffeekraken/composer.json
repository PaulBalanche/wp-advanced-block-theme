{
    "name": "wpextend/coffeekraken-add-on",
    "version": "1.0.0",
    "license": "proprietary",
    "authors": [
        {
            "name": "Paul Balanche",
            "email": "paul.balanche@gmail.com",
            "homepage": "https://github.com/PaulBalanche/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "coffeekraken/s-specs": "^2.0@alpha",
        "coffeekraken/s-frontspec": "^2.0@alpha",
        "coffeekraken/sugar": "^2.0@alpha"
    },
    "autoload": {
        "psr-4": {
            "Abt\\Coffeekraken\\": "inc/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "~/www/github.com/coffeekraken/packages/tools/s-specs",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "~/www/github.com/coffeekraken/packages/core/s-frontspec",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "~/www/github.com/coffeekraken/packages/tools/sugar",
            "options": {
                "symlink": false
            }
        }
    ]
}